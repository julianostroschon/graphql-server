type Login {
  token: String!
  user: User!
}

input LoginCredentials {
  password: String!
  username: String!
}

type Mutation {
  _empty: String!
  login(credentials: LoginCredentials!): Login!
}

type Query {
  _empty: String!
  me: User
  ping: String
  userGet(id: ID!): User
  usersLoad: [User!]!
}

type User {
  createdAt: String!
  email: String!
  id: ID!
  role: UserRole!
  updatedAt: String!
  username: String!
}

enum UserRole {
  ADMIN
  USER
}