{
  "name": "graphql-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "codegen": "graphql-codegen --config ./src/codegen/config.yml",
    "codegen:watch": "graphql-codegen --config ./src/codegen/config.yml --watch",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest --knexfile ./src/infra/database/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile ./src/infra/database/knexfile.ts",
    "seed:make": "knex seed:make -x ts",
    "seed:run": "knex seed:run --knexfile ./src/infra/database/knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.0.24",
    "@types/pg": "^8.15.4",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.12.1",
    "@graphql-codegen/plugin-helpers": "^5.1.1",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "change-case-all": "^2.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3"
  }
}
